{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": ["ES6", "DOM"],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "ES6",
    "noImplicitAny": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
